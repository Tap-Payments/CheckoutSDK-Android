<?xml version="1.0" encoding="utf-8"?>
<company.tap.tapuilibrary.uikit.atoms.TapChip xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tapCardChip5"
    android:layout_width="110dp"
    android:layout_height="40dp"
    app:cardCornerRadius="8dp"
    app:cardElevation="8dp"
    android:layout_marginTop="5dp"
    android:layout_gravity="center">
    <LinearLayout
        android:id="@+id/tapCardChip5Linear"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:orientation="horizontal">
    <mobi.foo.benefitinapp.utils.BenefitInAppButton
        android:id="@+id/checkout_btn"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:onClick="onButtonClicked"
        />
</LinearLayout>
</company.tap.tapuilibrary.uikit.atoms.TapChip>